  "terminal.integrated.minimumContrastRatio": 7,
  "terminal.integrated.scrollback": 99999,
  "terminal.integrated.shellIntegration.history": 999,
  "terminal.integrated.shellIntegration.suggestEnabled": true,
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.tabStopWidth": 2,
  "terminal.integrated.tabs.showActions": "always",
  "terminal.integrated.tabs.showActiveTerminal": "always",
  "window.autoDetectColorScheme": true,
  "workbench.colorTheme": "Tokyo Night Storm",
  "workbench.iconTheme": "file-icons",
  "workbench.preferredDarkColorTheme": "Tokyo Night",
  "workbench.preferredLightColorTheme": "Tokyo Night Storm",
  "workbench.productIconTheme": "flippidippi-product-icons",
