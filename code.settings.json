    "scss",
    "sass"
  ],
  "stylelint.validate": [
    "css",
    "less",
    "postcss",
    "scss",
    "sass"
  ],
  "svelte.enable-ts-plugin": true,
  "svelte.plugin.svelte.format.config.printWidth": 120,
  "svelte.plugin.svelte.format.config.singleQuote": true,
  "svelte.plugin.svelte.format.config.svelteBracketNewLine": false,
  "svelte.plugin.svelte.format.config.svelteStrictMode": true,
  "telemetry.telemetryLevel": "off",
  "terminal.external.windowsExec": "pwsh.exe",
  "terminal.integrated.allowMnemonics": true,
  "terminal.integrated.commandsToSkipShell": [
    "language-julia.interrupt"
